<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.farmfarm.updateCul">

	<insert id="updateCultivate" parameterType="Cultivating_product_detailVO">
		INSERT INTO cultivating_product_detail
		(product_serial_num,cultivate_status,cultivating_image,cultivating_content,fertilizer_content,
		 fertilizer_amount,etc_content)
		 VALUES
		 (#{product_serial_num},#{cultivate_status},#{cultivating_image},#{cultivating_content},#{fertilizer_content},
		 #{fertilizer_amount},#{etc_content})
	</insert>
	

</mapper>