<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.farmfarm.FarmersVO">
	<insert id="farmerInsert" parameterType="FarmersVO">
		INSERT INTO farmers(farmer_email, farmer_pw, farmer_name)
		VALUES(#{farmer_email},#{farmer_pw},#{farmer_name})
		
	</insert>
	
	<select id="getAllFarmers" resultType="FarmersVO">
		select * from farmers
	</select>

</mapper>
