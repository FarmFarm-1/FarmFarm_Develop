<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.farmfarm.term">

	<select id="getTermContent" parameterType="String" resultType="TermsOfUseVO">
		SELECT term_content 
		FROM terms_of_use 
		WHERE term_name = #{term_name}
	</select>

</mapper>
